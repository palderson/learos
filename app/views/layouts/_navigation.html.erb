<%= link_to "Learos", root_path, :class => 'brand' %>
<ul class="nav">
  <% if user_signed_in? %>
    <% if current_user.has_role? :admin %>
      <li>
        <%= link_to 'Admin', users_path %>
      </li>
    <% end %>
    <li><%= link_to 'Projects', projects_path %></li>
    <li class="dropdown">
      <a href="#" class="dropdown-toggle" data-toggle="dropdown">Integrations<b class="caret"></b></a>
      <ul class="dropdown-menu">
        <li><%= link_to 'JIRA', integrations_jira_index_path %></li>
      </ul>
    <% else %>
  <% end %>
</ul>
<ul class="nav pull-right">
  <% if user_signed_in? %>
    <li class="dropdown pull-right">
      <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class='icon-user'></i> <%= current_user.name %><b class="caret"></b></a>
      <ul class="dropdown-menu">
        <li><%= link_to 'View Profile', user_path(current_user) %>
        <li><%= link_to 'Edit Account', edit_user_registration_path %></li>
        <li><%= link_to 'Edit Subscription', edit_user_subscription_path(current_user, current_user.subscription) %></li>
        <li><%= link_to 'Logout', destroy_user_session_path, :method=>'delete' %></li>
      </ul>
    </li>
    <li>
    </li>
  <% else %>
    <li>
      <%= link_to 'Login', new_user_session_path %>
    </li>

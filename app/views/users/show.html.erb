<h3>User</h3>
<%= filepicker_image_tag @user.filepicker_url, w: 160, h: 160, fit: 'clip' %>
<p>User: <%= @user.name %></p>
<p>Email: <%= @user.email if @user.email %></p>

<h3>Projects I'm Working On</h3>
<% current_user.projects.each do |project| %>
	<p><%= link_to project.title, project %></p>
<% end %>	
